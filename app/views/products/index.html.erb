<%= object_messages %>
<div id="elements-list" class="span-24 last">
  <% unless @products.empty? %>
    <table id="table-element-list" summary="<%= t ('product.index.summary') %>">
      <%= render :partial => 'header' %>
      <%= render :partial => 'product', :collection => @products %>
    </table>
  <% end %>
</div>
<%= new_item_button new_product_path, :title => t('product.new_item'), :image_alt => t('product.new_item'), :image_title => t('product.new_item') %>
<div id="pagination" class="span-24">
  <%= paginate(@products) %>
</div>
